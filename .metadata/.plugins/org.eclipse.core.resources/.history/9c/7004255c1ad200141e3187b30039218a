void Load_Store::load_constant_to_register(vector<LGadget*>& l_gadgets) {
	vector<LGadget*>::iterator it = l_gadgets.begin();
	for(; it != l_gadgets.end(); ++ it) {
		if ((*it)->is_gadget_valid()) {
			vector<Inst*> vec = (*it)->insts;
		}
	}
}

